      "@platform/*": ["platforms/ladbrokesDesktop/*", "platforms/ladbrokesMobile/*"],
      "@ladbrokesDesktop/*": ["platforms/ladbrokesDesktop/*"],
      "@ladbrokesMobile/*": ["platforms/ladbrokesMobile/*"],

      "@shared/*": ["app/shared/*"],
      "@core/*": ["app/core/*"],

      "@lazy-modules-module/*": ["platforms/ladbrokesDesktop/lazy-modules/*", "platforms/ladbrokesMobile/lazy-modules/*", "app/lazy-modules/*"],
      "@sharedModule/*": ["platforms/ladbrokesDesktop/shared/*", "platforms/ladbrokesMobile/shared/*", "app/shared/*"],
      "@coreModule/*": ["platforms/ladbrokesDesktop/core/*", "platforms/ladbrokesMobile/core/*", "app/core/*"],
