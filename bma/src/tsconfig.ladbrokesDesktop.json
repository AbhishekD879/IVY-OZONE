      "@questionEngine/*": ["app/questionEngine/*"],
      "@bpp/*": ["app/bpp/*"],
      "@bybHistoryModule/*": ["platforms/ladbrokesDesktop/lazy-modules/bybHistory/*", "platforms/ladbrokesMobile/lazy-modules/bybHistory/*", "app/lazy-modules/bybHistory/*"],
      "@rightColumnModule/*": ["platforms/ladbrokesDesktop/lazy-modules/rightColumn/*", "app/lazy-modules/rightColumn/*"],
      "@specialsModule/*": ["app/lazy-modules/specialsSportTab/*"],
