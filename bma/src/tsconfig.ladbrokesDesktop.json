    "app/quickbet/quickbet.module.ts",
    "app/quickbet-stream-bet/sb-quickbet.module.ts",
    "app/betHistory/bet-history.module.ts",
    "app/retail/retail.module.ts",
    "platforms/ladbrokesMobile",
    "app/vanillaInit/vanilla-init.module.ts",
    "platforms/coralMobile",
