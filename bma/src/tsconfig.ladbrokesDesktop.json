      "@uktote/*": ["app/ukTote/*"],
      "@uktote-lazy-load/*": ["platforms/ladbrokesDesktop/ukTote/*"],
      "@promotions/*": ["app/promotions/*"],
      "@euro/*": [ "app/euro/*" ],
      "@featured/*": ["app/featured/*"],
      "@yourcall/*": ["app/yourCall/*"],
      "@ss/*": ["app/ss/*"],
      "@sb/*": ["app/sb/*"],
      "@banners/*": ["platforms/ladbrokesDesktop/banners/*"],
      "@edp/*": ["app/edp/*"],
      "@racing/*": ["app/racing/*"],
      "@betslip/*": ["app/betslip/*"],
      "@bma/*": ["app/bma/*"],
      "@player-props/*": ["app/player-props/*"],
      "@desktop/*": ["platforms/ladbrokesDesktop/desktop/*"],
      "@inPlayLiveStream/*": ["platforms/ladbrokesDesktop/inPlayLiveStream/*"],
      "@oddsBoost/*": ["app/oddsBoost/*"],
      "@freebetsModule/*": ["platforms/ladbrokesDesktop/freebets/*", "platforms/ladbrokesMobile/freebets/*", "app/freebets/*"],
