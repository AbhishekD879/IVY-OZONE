      "@desktop/*": [ "platforms/coralDesktop/desktop/*" ],
      "@questionEngine/*": [ "app/questionEngine/*" ],
      "@bybHistoryModule/*": ["app/lazy-modules/bybHistory/*"],
      "@rightColumnModule/*": ["app/lazy-modules/rightColumn/*"],
      "@specialsModule/*": ["app/lazy-modules/specialsSportTab/*"],
    }
  },
  "angularCompilerOptions": {
    "preserveWhitespaces": false,
    "strictInjectionParameters": false,
    "strictTemplates": false,
    "strictInputAccessModifiers": false,
    "strictInputTypes": false,
    "strictDomEventTypes": false,
    "strictContextGenerics": false,
    
  }
}
