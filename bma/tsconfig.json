{
  "compileOnSave": false,
  "compilerOptions": {
    "strict": true,
    "noImplicitAny": false,
    "strictNullChecks": false,
    "strictFunctionTypes": false,
    "strictBindCallApply": false,
    "noImplicitThis": false,
    "strictPropertyInitialization": false,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "downlevelIteration": true,
    "module": "esnext", 
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "target": "ES2022",
    "useDefineForClassFields": false,
    "typeRoots": [
      "../../../node_modules/@types",
      "typings/"
    ],
    "lib": [
      "es2017",
      "ES2022",
      "dom",
      "es2018.promise",
      "dom.Iterable"
    ],
    "baseUrl": "./src",
    "paths": {
      "@root/*": ["*"],
      "@environment/*": ["environments/*"],

      "@app/*": [ "app/*" ],
      "@lazy-libs/*": [ "lazy-libs/*" ],
      "@platform/*": [ "app/*" ],
      "@coralMobile/*": [ "platforms/coralMobile/*" ],
      "@coralDesktop/*": [ "platforms/coralDesktop/*" ],
      "@ladbrokesMobile/*": [ "platforms/ladbrokesMobile/*" ],
      "@ladbrokesDesktop/*": [ "platforms/ladbrokesDesktop/*" ],
      "@desktopModule/*": [ "platforms/coralDesktop/desktop/*" ],

      "@vanillaInitModule/*": [ "app/vanillaInit/*" ],

      "@authModule/*": [ "app/auth/*" ],
      "@betHistoryModule/*": [ "app/betHistory/*" ],
      "@betslipModule/*": [ "app/betslip/*" ],
      "@bmaModule/*": [ "app/bma/*" ],
      "@coreModule/*": [ "app/core/*" ],
      "@retailModule/*": [ "app/retail/*" ],
      "@betpackModule/*": [ "app/betpackMarket/*" ],
      "@betpackReviewModule/*": [ "app/betpackReview/*" ],
      "@sharedModule/*": [ "app/shared/*" ],
      "@quickbetModule/*": [ "app/quickbet/*" ],
      "@quickbetStreamBetModule/*": ["app/quickbet-stream-bet/*"],
      "@quickDepositModule/*": [ "app/quick-deposit/*" ],

      "@inplayModule/*": [ "app/inPlay/*" ],
      "@featuredModule/*": [ "app/featured/*" ],
      "@yourCallModule/*": [ "app/yourCall/*" ],
      "@promotionsModule/*": [ "app/promotions/*" ],
      "@fiveASideShowDownModule/*": [ "app/fiveASideShowDown/*" ],
      "@euroModule/*": [ "app/euro/*" ],
      "@lottoModule/*": [ "app/lotto/*" ],
      "@racingModule/*": [ "app/racing/*" ],
      "@vsbrModule/*": [ "app/vsbr/*" ],
      "@sportModule/*": [ "app/sb/sport/*" ],
      "@sbModule/*": [ "app/sb/*" ],
      "@olympicsModule/*": [ "app/olympics/*" ],
      "@betFinderModule/*": [ "app/bf/*" ],
      "@toteModule/*": [ "app/tote/*" ],
      "@ukToteModule/*": [ "app/ukTote/*" ],
      "@edpModule/*": [ "app/edp/*" ],
      "@favouritesModule/*": [ "platforms/coralMobile/favourites/*", "app/favourites/*" ],
      "@bigCompetitionsModule/*": [ "app/bigCompetitions/*" ],
      "@connectModule/*": [ "app/connect/*" ],
      "@oddsBoostModule/*": [ "app/oddsBoost/*" ],
      "@lazy-modules-module/*": [ "app/lazy-modules/*" ],
      "@localeModule/*": [ "app/lazy-modules/locale/*" ],
      "@freebetsModule/*": [ "app/freebets/*" ],

      "@core/*": [ "app/core/*" ],
      "@shared/*": [ "app/shared/*" ],
      "@banners/*": [ "app/banners/*" ],
      "@bpp/*": [ "app/bpp/*" ],
      "@lazy-modules/*": [ "app/lazy-modules/*" ],
      "@eventVideoStream/*": ["app/lazy-modules/eventVideoStream/*"],
      "@uktote/*": [ "app/ukTote/*" ],
      "@uktote-lazy-load/*": ["app/ukTote/*"],
      "@promotions/*": [ "app/promotions/*" ],
      "@euro/*": [ "app/euro/*" ],
      "@featured/*": [ "app/featured/*" ],
      "@yourcall/*": [ "app/yourCall/*" ],
      "@ss/*": [ "app/ss/*" ],
      "@sb/*": [ "app/sb/*" ],
      "@edp/*": [ "app/edp/*" ],
      "@retail/*": [ "app/retail/*" ],
      "@retail-lazy-load/*": ["app/retail/*"],
      "@racing/*": [ "app/racing/*" ],
      "@betslip/*": [ "app/betslip/*" ],
      "@quickDeposit/*": [ "app/quick-deposit/*" ],
      "@bma/*": [ "app/bma/*" ],
      "@player-props/*": [ "app/player-props/*" ],
      "@oddsBoost/*": [ "app/oddsBoost/*" ],
      "@connect/*": [ "app/connect/*" ],
      "@freebets/*": [ "app/freebets/*" ],
