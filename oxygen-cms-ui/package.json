{
  "name": "oxygen-cms-ui",
  "version": "174.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "clean": "rm -rf ../src/main/resources/static/keystone/* dist/",
    "start": "ng serve cms --progress --live-reload true -c dev0 --public-host cms-localhost.coral.co.uk:4200",
    "startDev1": "ng serve --configuration=dev1 --host 0.0.0.0 --public-host cms-localhost.coral.co.uk:4200 --open",
    "startDev2": "ng serve --configuration=dev2 --host 0.0.0.0 --public-host cms-localhost.coral.co.uk:4200 --open",
    "local": "ng serve --configuration=local --host 0.0.0.0 --public-host cms-localhost.coral.co.uk:4200 --open",
    "coverage-report-sonar": "ng test --watch=false --code-coverage=true",
    "testWatch": "ng test cms -- --watch=true --sourceMap=false",
    "build": "node --max_old_space_size=4000 node_modules/@angular/cli/bin/ng build --aot=true --buildOptimizer=true --output-path=dist/",
    "test": "ng test --code-coverage=true",
    "lint": "tslint --project tsconfig.json --config tslint.json --exclude '**/public/*.ts'",
    "e2e": "ng e2e",
    "rm-generated": "rm -rf src/app/client/private/* && rm -rf src/app/client/private/*",
    "generate-private": "node node_modules/.bin/yaml2json ../src/main/resources/static/private.yaml > private.json && node node_modules/.bin/a4apigen -s private.json -o src/app/client/private/",
    "generate-public": "node node_modules/.bin/yaml2json ../src/main/resources/static/public.yaml > public.json && node node_modules/.bin/a4apigen -s private.json -o src/app/client/public/"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "9.0.4",
    "@angular/cdk": "9.1.0",
    "@angular/common": "9.0.4",
    "@angular/compiler": "9.0.4",
    "@angular/core": "9.0.4",
    "@angular/forms": "9.0.4",
    "@angular/material": "9.1.0",
    "@angular/platform-browser": "9.0.4",
    "@angular/platform-browser-dynamic": "9.0.4",
    "@angular/platform-server": "9.0.4",
    "@angular/router": "9.0.4",
    "@auth0/angular-jwt": "4.0.0",
    "@ng-select/ng-option-highlight": "0.0.4",
    "@ng-select/ng-select": "^3.7.3",
    "angular-tree-component": "8.5.6",
    "bootstrap": "4.5.0",
    "core-js": "3.6.4",
    "jquery": "3.4.1",
    "lodash": "4.17.5",
    "material-components-web": "5.1.0",
    "ng2-trim-directive": "2.3.3",
    "rxjs": "6.5.4",
    "rxjs-compat": "6.5.4",
    "sortablejs": "1.10.2",
    "tslib": "1.11.1",
    "zone.js": "0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.900.4",
    "@angular/cli": "^9.0.4",
    "@angular/compiler-cli": "9.0.4",
    "@angular/language-service": "9.0.4",
    "@types/jasmine": "3.5.8",
    "@types/jasminewd2": "2.0.8",
    "@types/lodash": "4.14.149",
    "@types/node": "13.9.0",
    "angular4-swagger-client-generator": "0.1.27",
    "codelyzer": "5.2.1",
    "copy-webpack-plugin": "5.1.1",
    "jasmine-core": "3.5.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "4.4.1",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage-istanbul-reporter": "2.1.1",
    "karma-jasmine": "3.1.1",
    "karma-jasmine-html-reporter": "1.5.2",
    "protractor": "5.4.3",
    "ts-node": "8.6.2",
    "tslint": "6.0.0",
    "typescript": "3.7.5",
    "yamljs": "0.3.0"
  }
}
