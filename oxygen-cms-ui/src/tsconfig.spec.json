{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "outDir": "../out-tsc/spec",
    "baseUrl": "./",
    "target": "es5",
    "types": [
      "jasmine",
      "node"
    ]
  },
  "files": [
    "test.ts"
  ],
  "include": [
    "**/*.spec.ts",
    "**/*.d.ts",
    "**/*.*.ts",
    "./polyfills.ts"
  ],
  "paths": {
    "@angular/*": [
      "../node_modules/@angular/*"
    ]
  },
  "angularCompilerOptions": {
    "enableIvy": false
  }
}
